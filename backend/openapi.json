  "info": {
    "title": "Cultivation App",
    "description": "Cultivation API",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearer": { "scheme": "bearer", "bearerFormat": "JWT", "type": "http" }
    },
    "schemas": {
      "SpeciesListResponseDto": { "type": "object", "properties": {} },
      "SpeciesResponseDto": {
        "type": "object",
        "properties": {
          "name": { "type": "string", "maxLength": 255 },
          "id": {
            "type": "number",
            "example": 1,
            "description": "Unique identifier of the species"
          }
        },
        "required": ["name", "id"]
      },
      "CreateSpeciesRequestDto": {
        "type": "object",
        "properties": { "name": { "type": "string", "maxLength": 255 } },
        "required": ["name"]
      },
      "UpdateSpeciesRequestDto": {
        "type": "object",
        "properties": { "name": { "type": "string", "maxLength": 255 } },
        "required": ["name"]
      },
      "LiquidCultureResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "example": 1,
            "description": "Unique identifier of the culture"
          },
          "name": {
            "type": "string",
            "example": "Mushroom mania",
            "description": "Name of the culture"
          },
          "inoculationDate": {
            "type": "string",
            "example": "2025-01-20T00:00:00.000Z",
            "description": "Date when culture was inoculated",
            "format": "date-time"
          },
          "contaminationStatus": {
            "type": "boolean",
            "example": true,
            "description": "Contamination status of the culture",
            "nullable": true
          },
          "speciesId": {
            "type": "number",
            "example": 1,
            "description": "Unique identifier of the species"
          }
        },
        "required": [
          "id",
          "name",
          "inoculationDate",
          "contaminationStatus",
          "speciesId"
